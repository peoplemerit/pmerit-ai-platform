# Dependencies (prevent vulnerable packages)
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json
__pycache__/
*.pyc
vendor/

# Build outputs
dist/
build/
.next/
.nuxt/
out/

# Environment files (prevent credential exposure)
.env
.env.local
.env.production
.env.development
*.env

# Database files (prevent data exposure)
*.db
*.sqlite
*.sqlite3

# Log files (prevent information disclosure)
logs/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS files
.DS_Store
Thumbs.db
desktop.ini

